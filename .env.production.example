# PostgreSQL Configuration
POSTGRES_DB=justsell
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_password_here

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
# Image-capable Gemini model for backdrop generation
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image
# Use Gemini embedding models only (e.g. gemini-embedding-001).
# Do NOT use deprecated text-embedding-004.
GEMINI_EMBEDDING_MODEL=gemini-embedding-001
# Moderation image fetch tuning
MODERATION_IMAGE_FETCH_CONCURRENCY=3
MODERATION_IMAGE_FETCH_TIMEOUT_MS=5000
# Fail startup when embeddings probe fails
EMBEDDINGS_FAIL_FAST=true
IMAGE_GEN_RATE_LIMIT_BURST=5
IMAGE_GEN_RATE_LIMIT_REFILL_MS=10000

# Frontend Configuration
NEXT_PUBLIC_API_URL=/api
# Optional override for browser WebSocket URL (if API is on a different host)
# NEXT_PUBLIC_WS_URL=wss://api.justsell.co.nz/ws

# Backend Configuration
# Comma-separated list of allowed frontend origins for CORS + WebSocket Origin checks
ALLOWED_ORIGINS=https://www.justsell.co.nz,https://justsell.co.nz

# AWS S3 (Image Storage)
AWS_S3_BUCKET=justsell-images-prod
AWS_REGION=ap-southeast-2
